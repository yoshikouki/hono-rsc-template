{
  "name": "hono-rsc-template",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && echo 'import handler from \"./rsc/index.js\"; export default { fetch: handler };' > dist/worker-entry.js",
    "preview": "wrangler dev --local --port 8787",
    "deploy": "bun run build && wrangler deploy",
    "check": "ultracite check",
    "fix": "ultracite fix"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.2.1",
    "hono": "^4.9.0",
    "react": "^19.0.0",
    "react-compiler-runtime": "^1.0.0",
    "react-dom": "^19.0.0",
    "rehype-react": "^8.0.0",
    "remark": "^15.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.0.0",
    "tailwindcss": "^4.2.1",
    "unified": "^11.0.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250214.0",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitejs/plugin-rsc": "latest",
    "babel-plugin-react-compiler": "^1.0.0",
    "typescript": "^5.8.0",
    "ultracite": "7.2.3",
    "vite": "^6.0.0",
    "vitest": "^3.0.0",
    "wrangler": "^4.0.0"
  }
